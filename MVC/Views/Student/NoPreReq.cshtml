
@{
    ViewBag.Title = "NoPreReq";
}
@Scripts.Render("~/bundles/bootstrap")
@Styles.Render("~/Content/css")

<h2>NoPreReq <button class="btn btn-defaul" id="backToMenu" onclick="window.location.href = '/Student?id=A000001'"> Back to Menu </button></h2>

<h4>Warning ! Prerequsite not fullfilled</h4>
<h4>Request Prerequsite Override?</h4>
<div class="modal-dialog">
    <div class="modal-content">
        <!-- Modal Body -->
        <div class="modal-body" id="divNoPreReq">
            <form class="form-horizontal" method="post" name="prereqform" action="~/Student/EnrollCourse?id=@ViewBag.id">

                <div>
                    <input type="submit" class="btn btn-primary" value="Request" id="idSubmitButton"  />
                    <input type="submit" class="btn btn-primary" value="Cancel" id="idSubmitButton" />
                </div>

            </form>
        </div>
    </div>
</div>

